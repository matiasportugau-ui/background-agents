# Environment Configuration for Background Agents

# Application
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Database (if needed)
DATABASE_URL=mongodb://localhost:27017/background-agents

# Security
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here

# External Services
API_BASE_URL=https://api.example.com
API_KEY=your-api-key-here

# Agent-specific configurations
MONITOR_INTERVAL=*/5 * * * *
PROCESSOR_BATCH_SIZE=100
MAX_RETRIES=3
RETRY_DELAY=1000
